[supervisor]
nodaemon=true

[program:nginx]
command=nginx -g "daemon off;"
autostart=true

[program:backend]
command=node /app/Backend/index.js
directory=/app/Backend
autostart=true
environment=
    PORT=5000,
    AWS_ACCESS_KEY_ID="%(ENV_AWS_ACCESS_KEY_ID)s",
    AWS_SECRET_ACCESS_KEY="%(ENV_AWS_SECRET_ACCESS_KEY)s"

[program:ml]
command=python /app/ml/ml_model_api.py
directory=/app/ml
autostart=true
environment=
    PYTHONUNBUFFERED=1